		System.out.println("논리 연산을 입력하세요>>");
		boolean a = sc.nextBoolean();
		String op = sc.next();
		boolean b = sc.nextBoolean();
		switch(op) {
		case "AND" :
			System.out.println(a && b);
			break;
		case "OR" :
			System.out.println(a || b);
