package classtest;

public class Person {
	String name;
	int height;
	int weight;
	char gender;
	boolean married;
	
	public void showPersonInfo() {
		System.out.println(add(10,20));
		System.out.println(name +","+ height +"," + weight+ ","+gender+","+married);
	}
	//반환 자료형  메서드명(매개변수) {
	//	실행문;
	//	return 반환값;
//}
	// void : 값을 반환하지 않을 때 사용.
	int add(int x, int y) {
		int result = x+y;
		return result;
	}
}
